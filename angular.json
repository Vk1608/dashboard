{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "kubernetes-dashboard": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": ".tmp/frontend",
            "index": "src/app/frontend/index.html",
            "main": "src/app/frontend/index.ts",
            "tsConfig": "src/app/frontend/../../../aio/tsconfig.app.json",
            "polyfills": "src/app/frontend/polyfills.ts",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/app/frontend/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/xterm/dist/xterm.css",
              "node_modules/c3/c3.min.css",
              "src/app/frontend/index.scss"
            ],
            "scripts": [
              "node_modules/sockjs-client/dist/sockjs.min.js",
              "node_modules/systemjs/dist/s.js",
              "node_modules/systemjs/dist/extras/named-register.js",
              "node_modules/systemjs/dist/extras/amd.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/app/frontend/environments/environment.ts",
                  "with": "src/app/frontend/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "kubernetes-dashboard:build",
            "port": 8080
          },
          "configurations": {
            "production": {
              "browserTarget": "kubernetes-dashboard:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "kubernetes-dashboard:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/app/frontend/index.spec.ts",
            "karmaConfig": "aio/karma.conf.js",
            "polyfills": "src/app/frontend/polyfills.ts",
            "tsConfig": "src/app/frontend/../../../aio/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "node_modules/material-design-icons/iconfont/material-icons.css",
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/c3/c3.min.css",
              "src/app/frontend/index.scss"
            ],
            "assets": [
              "src/app/frontend/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "kubernetes-dashboard-e2e": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "aio/protractor.conf.js",
            "devServerTarget": "kubernetes-dashboard:serve",
            "webdriverUpdate": false
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    },
    "plugin": {
      "projectType": "library",
      "root": "projects/plugin",
      "sourceRoot": "projects/plugin/src",
      "prefix": "k8s",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/plugin/tsconfig.lib.json",
            "project": "projects/plugin/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/plugin/src/test.ts",
            "tsConfig": "projects/plugin/tsconfig.spec.json",
            "karmaConfig": "projects/plugin/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/plugin/tsconfig.lib.json",
              "projects/plugin/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "custom-plugin": {
      "root": "projects/custom/",
      "sourceRoot": "projects/custom/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "./builders:plugin",
          "options": {
            "outputPath": "dist/plugins",
            "index": "",
            "main": "projects/custom/src/main.ts",
            "polyfills": "projects/custom/src/polyfills.ts",
            "tsConfig": "projects/custom/tsconfig.app.json",
            "assets": [],
            "styles": [],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        }
      }
    }
  },
  "defaultProject": "kubernetes-dashboard",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "kd",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "kd"
    }
  }
}
